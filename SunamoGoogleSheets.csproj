<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>24.1.8.1</Version>
	  <Description>Parsing from and formatting to Google Sheets</Description>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="SunamoBts" Version="24.1.5.1" />
    <PackageReference Include="SunamoCollections" Version="24.1.7.1" />
    <PackageReference Include="SunamoCollectionsGeneric" Version="24.1.5.1" />
    <PackageReference Include="SunamoStringGetLines" Version="24.1.7.1" />
    <PackageReference Include="SunamoStringJoin" Version="24.1.5.1" />
    <PackageReference Include="SunamoStringSplit" Version="24.1.5.1" />
  </ItemGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
</Project>